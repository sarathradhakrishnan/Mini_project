<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Roll Number</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Class</th>
      <th scope="col" class="text-end">Action</th>
    </tr>
  </thead>
  <tbody>

    {{#each rows}}
    <tr>
      <th scope="row">{{this.Rollno}}</th>
      <td>{{this.First_Name}}</td>
      <td>{{this.Last-Name}}</td>
      <td>{{this.Email}}</td>
      <td>{{this.Phone}}</td>
      <td>{{this.class}}</td>
      <td class="text-end">
          <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-eye"></i>View</a>
          <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-pencil"></i>Edit</a>
          <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-person-x"></i>Delete</a>
      </td>
    </tr>
    {{/each}}

  </tbody>
</table>